{
  "schemaVersion": 1,
  "id": "voxxic",
  "version": "${version}",
  "name": "Voxxic",
  "description": "Voxxology is the study of the particles that make up the blocky world of Minecraft. In these very particles holds the strings that tie reality together, the very essence of existence. Though I wonder what lies between the seams and what kind of wonders I may learn from it.",
  "authors": [
    "TwinSouls",
    "TheCorrupted"
  ],
  "contact": {
    "homepage": "https://example.com/",
    "issues": "https://github.com/SaltSouls/Voxxic/issues",
    "sources": "https://github.com/SaltSouls/Voxxic"
  },
  "license": "GNU Lesser General Public License version 3 (LGPLv3)",
  "icon": "assets/voxxic/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.tainted.voxxic.core.Voxxic"
      }
    ],
    "client":  [
      {
        "adapter": "kotlin",
        "value": "com.tainted.voxxic.client.VOXClient"
      }
    ]
  },
  "mixins": [
    "voxxic.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.6",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.6.3+kotlin.1.5.21",
    "minecraft": "1.17.x",
    "java": ">=16"
  }
}
